<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.flnet.operationsystem.dao.mapper.CityMapper">

<select id="selectCityById" resultType="com.flnet.operationsystem.entity.City">
	select cid,cityName as name,des from city where cid=#{cid}
</select>

</mapper>